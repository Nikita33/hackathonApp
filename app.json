{
  "expo": {
    "name": "ChannelPartners",
    "slug": "channel-partner-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/compass-app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/compass-splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.channelpartner.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/compass-adaptive-icon.png",
        "backgroundColor": "#1E5EF6"
      },
      "package": "com.channelpartner.app"
    },
    "web": {
      "favicon": "./assets/compass-app-icon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
